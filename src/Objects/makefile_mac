	gnatmake -c $(INCLULIBS) $(GNATOPTFLAGS) use_c2phc.adb
	g++ -Dcompilewgpp=1 -c $(PHCLib)/syscon.c
	g++ -Dcompilewgpp=1 -c $(PHCLib)/lib_syscon.c
	gnatbind -n use_c2phc
	gnatlink -C use_c2phc syscon.o lib_syscon.o -o $(BIN)/lib_syscon \
                 --LINK=g++

