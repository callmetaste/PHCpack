	gnatlink -C use_c2phc phcpack.o solcon.o syscon.o witset.o \
                    jump_track.o parallel_phcpack.o parallel_monodromy.o \
                    mpi2fac_s.o \
                 -o ../bin/mpi2fac_s --LINK=$(CLINKER)

mpi2fac_d:
